plot(y,type = "o",col = "red", xlab = "Classifier", ylab = "F1-score", main = "English-Italian",
ylim=c(0.60,1), xlim=c(0,11))
lines(y1, type = "o", col = "blue")
textxy(x, y, lb)
textxy(x, y1, lb)
